{
  "name": "git-groom",
  "version": "0.1.3",
  "description": "A CLI tool for cleaning up Git branches, tags, and remotes",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sirrine-Jonathan/git-groom.git"
  },
  "main": "src/index.js",
  "bin": {
    "git-groom": "bin/cli.js"
  },
  "type": "module",
  "scripts": {
    "start": "node bin/cli.js",
    "deploy": "npm version patch && npm publish --access public",
    "prettier": "prettier --write '**/*.{js,md,json}'"
  },
  "keywords": [
    "git",
    "cleanup",
    "branches",
    "tags",
    "remotes"
  ],
  "author": "Jonathan Sirrine",
  "email": "sirrineprogramming@gmail.com",
  "license": "ISC",
  "dependencies": {
    "commander": "^13.1.0",
    "execa": "^9.5.2",
    "inquirer": "^12.4.1",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "prettier": "^3.4.2"
  },
  "engines": {
    "node": ">=16"
  }
}
